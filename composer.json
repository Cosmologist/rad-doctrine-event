{
    "name": "knplabs/rad-doctrine-event",
    "description": "Access your doctrine events from the Symfony DIC.",
    "license": "MIT",
    "authors": [
        {
            "name": "KNPLabs Team",
            "email": "hello@knplabs.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "doctrine/inflector": "~1.0",
        "doctrine/orm": "~2.0",
        "symfony/config": "~2.0",
        "symfony/dependency-injection": "~2.0",
        "symfony/http-kernel": "~2.0"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.1",
        "knplabs/phpspec-welldone-extension": "dev-master@dev"
    },
    "autoload": {
        "psr-0": {
            "Knp\\Rad\\DoctrineEvent": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
